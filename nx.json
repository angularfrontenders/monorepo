{
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/dist"],
      "cache": true
    },
    "test": {
      "dependsOn": ["^test"],
      "outputs": ["{projectRoot}/coverage"],
      "inputs": ["default", "^default", "{workspaceRoot}/karma.conf.js"],
      "cache": true
    },
    "lint": {
      "dependsOn": ["^lint"],
      "inputs": ["default", "{workspaceRoot}/.eslintrc.json"],
      "cache": true
    },
    "@nx/eslint:lint": {
      "dependsOn": ["^lint"],
      "inputs": ["default", "{workspaceRoot}/.eslintrc.json"],
      "cache": true
    }
  },
  "defaultBase": "master",
  "generators": {
    "@nx/angular:application": {
      "style": "css",
      "linter": "eslint",
      "e2eTestRunner": "none"
    },
    "@nx/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "none"
    },
    "@nx/angular:component": {
      "style": "css"
    }
  },
  "defaultProject": "identifiers"
}
