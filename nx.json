{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "lint"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "outputs": [
        "{projectRoot}/coverage"
      ],
      "inputs": [
        "default",
        "^default",
        "{workspaceRoot}/karma.conf.js"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json"
      ]
    }
  },
  "defaultBase": "master",
  "generators": {
    "@nx/angular:application": {
      "style": "css",
      "linter": "eslint",
      "e2eTestRunner": "none"
    },
    "@nx/angular:library": {
      "linter": "eslint"
    },
    "@nx/angular:component": {
      "style": "css"
    }
  },
  "defaultProject": "identifiers"
}
